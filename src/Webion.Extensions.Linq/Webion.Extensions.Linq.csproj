<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        
        <Version>1.0.2</Version>
        <IsPackable>true</IsPackable>
        <OutputType>Library</OutputType>

        <Title>Webion.Extensions.Linq</Title>
        <Authors>Webion SRL</Authors>
        <Description>
            Contains various extension methods used for operations on IEnumerables, IQueryables and ICollections.
        </Description>
        <PackageLicense>https://licenses.nuget.org/MIT</PackageLicense>
        <PackageLicenseUrl>https://licenses.nuget.org/MIT</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/webion-hub/Webion.Extensions</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <TargetFrameworks>net7.0;net8.0</TargetFrameworks>
        <LangVersion>12</LangVersion>
        <UserSecretsId>a74b5c59-3349-4912-8142-98d18a673980</UserSecretsId>
    </PropertyGroup>

    <PropertyGroup>
        <PackageReadmeFile>README.md</PackageReadmeFile>
    </PropertyGroup>

    <ItemGroup>
        <None Include="README.md" Pack="true" PackagePath="\"/>
    </ItemGroup>


    <ItemGroup>
        <ProjectReference Include="..\Webion.Extensions.Linq.Abstractions\Webion.Extensions.Linq.Abstractions.csproj" />
    </ItemGroup>
</Project>
